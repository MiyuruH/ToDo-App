{
  "expo": {
    "name": "ToDo-App",
    "slug": "ToDo-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/taskX.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/taskX.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/taskX.png",
        "backgroundColor": "#ffffff"
      }
    },
    "scheme": "ToDo-App",
    "web": {
      "favicon": "./assets/taskX.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
